{
  "name": "Test Extension",
  "version": "0.0",
  "description": "Just load and execute a WASM file on every page",
  "manifest_version": 2,

  "background": {
      "persistent": false,
      "scripts": [ "worker_proxy.js" ]
  },
  "content_scripts": [{
      "matches": [ "*://*/*" ],
      "js": [
          "worker_proxy.js",
          "lib.js"
      ]
  }],
  "web_accessible_resources": [ "worker_proxy.html", "lib.wasm" ]
}
